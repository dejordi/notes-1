16.5
procedure Lower(T)
   a := 1
   while T[a][a] != nil
      k := a - 1
      while k > 0
         T[a][k]
         k := k - 1
      a := a + 1

16.6
procedure Min2D(T)
   minimum := T[1][1]
   a := 1
   b := 1
   while T[a][b] != nil
      while T[a][b] != nil
         if T[a][b] < minimum
            minimum  := T[a][b]
         b := b + 1
      b := 1
      a := a + 1
   return minimum
