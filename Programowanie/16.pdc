16.1
function Rows(T)
   c := 1
   while T[c][0] != nil
      c := c + 1
   return c - 1

16.2
function Columns(T)
   c := 1
   while(T[0][c] != nil)
      c := c + 1
   return c - 1

16.3
procedure Unit(T)
   a := 1
   b := 1
   while T[a][b] != nil
      while T[a][b] != nil
         T[a][b] := 0
         b := b + 1
      b := 1
      a := a + 1

   for i := 1 to a
      T[i][i] := 1

16.4
function Diagonal(T)
   sum := T[1][1]
   a := 2
   while T[a][a] != nil
      sum := sum + T[a][a]
   return sum

